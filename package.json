{
  "name": "api-rest-mock",
  "version": "1.0.0",
  "description": "Service for mocking API",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    "./models/*": "./src/models/*.js",
    "./inbound/*": "./src/inbound/*.js",
    "./outbound/*": "./src/outbound/*.js",
    "./utils/*": "./src/utils/*.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "start:watch": "node --watch src/index.js",
    "test": "node --test",
    "coverage": "npm test -- --experimental-test-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabian-parra/api-rest-mock.git"
  },
  "keywords": [
    "mock",
    "api",
    "test"
  ],
